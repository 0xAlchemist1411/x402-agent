{
  "name": "x402-agent",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev:api": "tsx --watch src/server.ts",
    "dev:mcp": "tsx --watch src/mcp.ts",
    "dev": "concurrently \"npm:dev:api\" \"npm:dev:mcp\"",
    "start:api": "NODE_ENV=production tsx src/server.ts",
    "start:mcp": "NODE_ENV=production tsx src/mcp.ts",
    "start": "concurrently \"npm:start:api\" \"npm:start:mcp\"",
    "lint": "eslint . --ext .ts,.tsx",
    "fmt": "prettier --write .",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "seed": "npx prisma db seed",
    "test": "echo \"No tests configured\" && exit 0"
  },
  "displayName": "Node Server",
  "description": "Node.js script with gill (based on @solana/kit)",
  "usecase": "Backend",
  "keywords": [
    "gill",
    "node",
    "solana-kit"
  ],
  "version": "1.0.0",
  "main": "dist/index.js",
  "type": "module",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/cors": "^2.8.19",
    "@types/node": "^24.5.2",
    "@types/prompts": "^2.4.9",
    "cross-env": "^10.0.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.35.0",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "prisma": "^6.18.0",
    "tsx": "^4.20.6",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.44.0"
  },
  "dependencies": {
    "@atxp/express": "^0.8.2",
    "@modelcontextprotocol/sdk": "^1.21.0",
    "@prisma/client": "^6.19.0",
    "@types/axios": "^0.9.36",
    "@types/express": "^5.0.5",
    "@types/multer": "^2.0.0",
    "axios": "^1.13.1",
    "bignumber.js": "^9.3.1",
    "cors": "^2.8.5",
    "esrun": "^0.0.1",
    "express": "^5.1.0",
    "gill": "0.11.0",
    "kleur": "^4.1.5",
    "multer": "^2.0.2",
    "prompts": "^2.4.2",
    "ws": "^8.18.3",
    "zod": "^3.25.76"
  }
}
